{
  "name": "uncharted-lands",
  "description": "Uncharted Lands is a game where players must build and manage settlements in a procedurally generated world. They must overcome various challenges such as extreme weather conditions, scarce resources, and hostile creatures, all while expanding their settlements and improving their technology.",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "prisma generate && vite build",
    "migrate": "prisma migrate deploy",
    "preview": "vite preview",
    "test": "playwright test",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.4.4",
    "@mdi/font": "^7.2.96",
    "@mdi/js": "^7.2.96",
    "@playwright/test": "^1.35.1",
    "@sentry/node": "^7.57.0",
    "@sentry/svelte": "^7.57.0",
    "@sentry/tracing": "^7.57.0",
    "@sentry/vite-plugin": "^0.4.0",
    "@skeletonlabs/skeleton": "^1.9.0",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/svelte": "^3.2.2",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.16.19",
    "@types/node-cron": "^3.0.8",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/coverage-c8": "^0.28.5",
    "alea": "^1.0.1",
    "autoprefixer": "^10.4.14",
    "bcrypt": "^5.0.1",
    "bson": "^4.7.2",
    "cuid": "^3.0.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "fractal-noise": "^2.1.0",
    "jest-dom": "^4.0.0",
    "jsdom": "^21.1.2",
    "mdi-svelte": "^1.1.2",
    "mdsvex": "^0.10.6",
    "node-cron": "^3.0.2",
    "open-simplex-noise": "^2.5.0",
    "postcss": "^8.4.25",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "^4.16.2",
    "redis": "^4.6.7",
    "simplex-noise": "^4.0.1",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "sveld": "^0.18.1",
    "svelte-canvas": "^0.8.1",
    "svelte-check": "^2.10.3",
    "svelte-material-icons": "^2.1.0",
    "svelte-preprocess": "^5.0.4",
    "tabulator-tables": "^5.5.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "@sveltejs/kit": "^1.22.1",
    "svelte": "^3.59.2"
  },
  "type": "module"
}
